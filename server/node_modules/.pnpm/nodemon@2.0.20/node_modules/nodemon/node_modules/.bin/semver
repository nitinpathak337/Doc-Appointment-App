#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/doc-appointment-app/server/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/semver/node_modules:/doc-appointment-app/server/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/doc-appointment-app/server/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/doc-appointment-app/server/node_modules/.pnpm/semver@5.7.1/node_modules:/doc-appointment-app/server/node_modules/.pnpm/node_modules:/doc-appointment-app/server/node_modules:/doc-appointment-app/node_modules:/node_modules:/doc-appointment-app/server/node_modules/.pnpm/nodemon@2.0.20/node_modules/nodemon/node_modules:/doc-appointment-app/server/node_modules/.pnpm/nodemon@2.0.20/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
else 
  exec node  "$basedir/../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
fi
